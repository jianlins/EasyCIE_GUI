<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER_LEFT" minHeight="30.0" minWidth="400.0" prefHeight="30.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" 
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.utah.bmi.simple.gui.controller.BottomViewController">
   <children>
      <Label fx:id="status" prefHeight="16.0" minWidth="70" text="Status:  " layoutY="-10"/>
      <Label fx:id="msg"  minWidth="180.0" prefWidth="200.0" text="    " />
      <Separator orientation="VERTICAL" prefWidth="7.0" />
      <ProgressBar fx:id="progressBar" prefWidth="1000.0" progress="0.0" />
      <Separator orientation="VERTICAL" prefWidth="15.0" />
      <HBox alignment="CENTER_RIGHT" minWidth="200">
         <children>
            <Button fx:id="resetButton" mnemonicParsing="false" prefWidth="100.0" text="Reset" />
            <Button fx:id="saveButton" mnemonicParsing="false" prefWidth="100.0" text="Save" />
            <Button fx:id="cancelButton" mnemonicParsing="false" prefWidth="100.0" text="Cancel" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</HBox>
